{
    "name": "preact-vite-starter",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite",
        "build": "cross-env NODE_ENV=production vite build",
        "prettier": "prettier --write '**/*.{js,jsx,css,html}'",
        "lint": "eslint --fix src/**/*.tsx"
    },
    "dependencies": {
        "preact": "10.5.14",
        "wouter": "2.7.5"
    },
    "devDependencies": {
        "@iconify/json": "1.1.400",
        "@preact/preset-vite": "2.1.0",
        "@typescript-eslint/eslint-plugin": "4.30.0",
        "@typescript-eslint/parser": "4.30.0",
        "cross-env": "7.0.3",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "3.4.1",
        "eslint-plugin-promise": "5.1.0",
        "husky": "7.0.2",
        "lint-staged": "11.1.2",
        "prettier": "2.4.1",
        "typescript": "4.4.3",
        "vite": "2.5.6",
        "vite-plugin-pwa": "0.11.2",
        "vite-plugin-windicss": "1.4.2",
        "windicss": "3.1.8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*/**/*.{js,jsx,ts,tsx, json}": [
            "npx prettier --write"
        ]
    },
    "peerDependencies": {
        "postcss": "^8.3.6"
    },
    "resolutions": {
        "postcss": "8.1.10"
    }
}
